{
  "name": "piotrpress/private-composer",
  "type": "composer-plugin",
  "license": "MIT",
  "description": "This composer plugin adds github and bitbucket protocols support to Composer in order to simplify private repositories handling.",
  "homepage": "https://github.com/PiotrPress/private-composer",
  "authors": [
    {
      "name": "Piotr Niewiadomski",
      "homepage": "https://piotr.press"
    }
  ],
  "keywords": [
    "composer", "private", "private-repository", "private-packagist", "packagist", "composer plugin", "stream", "streams", "github", "bitbucket", "protocols", "php"
  ],
  "require": {
    "php": ">=7.4",
    "composer-plugin-api": "^2.0",
    "piotrpress/streamer": "^1.0",
    "piotrpress/remoter": "^1.0"
  },
  "extra": {
    "class": "PiotrPress\\PrivateComposer\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "PiotrPress\\PrivateComposer\\": "src/"
    },
    "classmap": [
      "src"
    ]
  }
}